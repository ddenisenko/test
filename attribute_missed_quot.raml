              				<popularity>0.28</popularity>
              			</artist>
              		</searchResult>
              	</searchResults>
              </response>
                      
          application/json:
            example: |  
              {
                  "status": "ok",
                  "version": "1.2",
                  "searchResults": {
                      "page": 1,
                      "pageSize": 2,
                      "totalItems": 244,
